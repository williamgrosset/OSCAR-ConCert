								<xsd:enumeration value="024-05"/>	<!-- Psychiatric Hospital -->
								<xsd:enumeration value="024-06"/>	<!-- Other Specialty Hospital -->
								<xsd:enumeration value="024-07"/>	<!-- No fixed address -->
								<xsd:enumeration value="024-08"/>	<!-- Hostel, Shelter -->
								<xsd:enumeration value="024-09"/>	<!-- Long term care facility, Nursing Home -->
								<xsd:enumeration value="024-10"/>	<!-- Municipal Non-Profit Housing -->
								<xsd:enumeration value="024-11"/>	<!-- Private Non-Profit Housing -->
