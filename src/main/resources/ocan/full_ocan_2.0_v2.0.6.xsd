<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsd:schema targetNamespace="http://oscarehr.org/ocan" xmlns:ocan="http://oscarehr.org/ocan" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified">

	<!-- 
	============================================================================================
	Program Name	: full_ocan_v2.0.6.xsd 
	Description		: XML schema for the file submission of FULL OCAN assessment tool
	Developed by	: Khaled Awad
	History : 
			2.0.0 : 04-Mar-2010 initial version
			2.0.1 : 06-Apr-2010 revise column length and add few more fields
			2.0.2 : 03-May-2010 breakdown contact information into multiple columns
			2.0.3 : 14-May-2010 change UTF-16 to UTF-8
			2.0.4 : 20-May-2010 make client name optional
			2.0.5 : 04-Jun-2010 remove months from ageOnsetMental,ageHospitalization and review None code 
			2.0.6 : 23-Jun-2010 allow null for "accepted" and add more functional centres
	============================================================================================
	-->

	<xsd:element name="OCANv2SubmissionFile">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence maxOccurs="unbounded">
						<xsd:element ref="ocan:OCANv2SubmissionRecord"/>
					</xsd:sequence>

					<!-- OCAN Tool Version Number: it has to be a constant 2.0.5 -->
					<xsd:attribute name="tool_revision" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="2.0.5"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Schema Version Number: it has to be a constant 2.0.6 -->
					<xsd:attribute name="schema_version" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="2.0.6"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- File ID -->
					<xsd:attribute name="ID" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Timestamp: on which the File is generated YYYY-MM-DDThh:mi:ssZ -->
					<xsd:attribute name="timestamp" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3}))-((0[1-9])|(1[012]))-((0[1-9])|([12][0-9])|(3[01]))T(([01][0-9])|(2[0-3]))(:[0-5][0-9]){2}(\.[0-9]+)?(([\+|\-]((0[0-9])|(1[0-2]))(:[0-5][0-9]))|(\+13(:[0-5][0-9])(:[0-5][0-9]))|\+14:00|Z)"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

			</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="OCANv2SubmissionRecord">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence>
						<xsd:element ref="ocan:submitOrganizationRecord"/>
						<xsd:element ref="ocan:clientRecord"/>
						<xsd:element ref="ocan:OCANDomains"/>
						<xsd:element ref="ocan:additionalElements"/>
					</xsd:sequence>

					<!-- 
						There is a limitation regarding types such as xsd:dateTime and xsd:date in SQL Server 2005 
						as well as their derived types. SQL Server 2005 always requires a timezone (whereas the XSD 
						standard says values with or without a time zone should be accepted).
						For this reason the following fields in this schema MUST BE suffixed by Z. And so they will
						be formated as "YYYY-MM-DDZ". the fields are startDate, completionDate, clientDOB,
						requestForServiceDate, serviceDecisionDate, serviceInitiationDate, exitDate, reviewDate. 
					-->

					<!-- OCAN Type: whether it is full, self and core, in this schema it will be a constant: FULL  -->
					<xsd:attribute name="OCANType" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="FULL"/>	<!-- Full OCAN -->
								<xsd:enumeration value="SELF" />	<!-- Core + Self OCAN -->
								<xsd:enumeration value="CORE"/>	<!-- Core OCAN -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Assessment Start Date: YYYY-MM-DDZ note the existence of the letter Z at the end of date value -->
					<xsd:attribute name="startDate" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3}))-((0[1-9])|(1[012]))-((0[1-9])|([12][0-9])|(3[01]))(Z)"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Assessment Completion Date: YYYY-MM-DDZ note the existence of the letter Z at the end of date value -->
					<xsd:attribute name="completionDate" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:pattern value="((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3}))-((0[1-9])|(1[012]))-((0[1-9])|([12][0-9])|(3[01]))(Z)"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Assessment ID: it has to be unique accross the file -->
					<xsd:attribute name="assessmentID" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Assessment Revision Number: this has been added to support IAR, if vendor software does not have it, then default it to 1 -->
					<xsd:attribute name="assessmentRevision" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="12">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Assessment Status: only Completed assessments will be included in data submission -->
					<xsd:attribute name="assessmentStatus" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="Complete"/>	<!-- Completed -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!--
						This question does not appear on the Assessment. However, consent should collected 
						as per Organizational Policies around Consent. This consent pertains to viewing of 
						the Assessment on IAR only. It does not pertain to the submission of the Assessment 
						Record to IAR. All completed Assessments are to be submitted to IAR. The system should 
						have the ability to record consent outside of each Assessment but should be included 
						in the Assessment record as part of the data submission.
		
						Valid Values:
							- TRUE = Yes: Assessment is sent to IAR and is available for viewing.
							- FALSE = No: Assessment is sent to IAR and NOT available for viewing.
					-->
					<xsd:attribute name="IARViewingConsent" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="submitOrganizationRecord">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element ref="ocan:submitOrg"/>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="submitOrg">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Submitting Organization Name -->
					<xsd:attribute name="name" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="128">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Submitting Organization Number -->
					<xsd:attribute name="number" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="4">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="clientRecord">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>

						<xsd:element ref="ocan:clientID"/>
						<xsd:element ref="ocan:completedByOCANLead"/>
						<xsd:element ref="ocan:reasonForOCAN"/>
						<xsd:element ref="ocan:consumerSelfAxCompleted"/>
						<xsd:element ref="ocan:reasonConsumerSelfAxNotCompletedList" minOccurs="1"/>
						<xsd:element ref="ocan:clientName"/>
						<xsd:element ref="ocan:clientAddress"/>
						<xsd:element ref="ocan:clientPhone"/>
						<xsd:element ref="ocan:clientEmailAddress"/>
						<xsd:element ref="ocan:clientDOB"/>
						<xsd:element ref="ocan:clientHealthCardInfo"/>
						<xsd:element ref="ocan:serviceRecipientLocation"/>
						<xsd:element ref="ocan:serviceRecipientLHIN"/>
						<xsd:element ref="ocan:gender"/>
						<xsd:element ref="ocan:maritalStatus"/>
						<xsd:element ref="ocan:serviceUseRecordList" minOccurs="1" maxOccurs="1"/>
						<xsd:element ref="ocan:clientContact"/>
						<xsd:element ref="ocan:clientCapacity"/>
						<xsd:element ref="ocan:ageOnsetMental"/>
						<xsd:element ref="ocan:ageHospitalization"/>
						<xsd:element ref="ocan:firstEntryDate"/>
						<xsd:element ref="ocan:clientCulture"/>
						<xsd:element ref="ocan:aboriginalOrigin"/>
						<xsd:element ref="ocan:citizenshipStatus"/>
						<xsd:element ref="ocan:timeLivedInCanada"/>
						<xsd:element ref="ocan:immigExpList" minOccurs="0"/>
						<xsd:element ref="ocan:immigExpFreeText"/>
						<xsd:element ref="ocan:discrimExpList" minOccurs="0"/>
						<xsd:element ref="ocan:prefLang"/>
						<xsd:element ref="ocan:serviceLang"/>
						<xsd:element ref="ocan:legalIssues"/>
						<xsd:element ref="ocan:legalStatusList" minOccurs="0"/>
						<xsd:element ref="ocan:generalComments"/>

					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client ID: this should be unique within the organization -->
	<xsd:element name="clientID">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence/>
					<xsd:attribute name="orgClientID" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- OCAN completed by OCAN Lead? -->
	<xsd:element name="completedByOCANLead">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="TRUE"/>	<!-- Yes -->
				<xsd:enumeration value="FALSE"/>	<!-- No -->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<!-- Reason for OCAN (select one) -->
	<xsd:element name="reasonForOCAN">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="IA"/>		<!-- Initial OCAN -->
								<xsd:enumeration value="RA"/>		<!-- Reassessment -->
								<xsd:enumeration value="DIS"/>		<!-- (Prior to) discharge -->
								<xsd:enumeration value="SC"/>		<!-- Significant Change -->
								<xsd:enumeration value="REV"/>		<!-- Review -->
								<xsd:enumeration value="REK"/>		<!-- Re-key -->
								<xsd:enumeration value="OTHR"/>	<!-- Other (Consumer request) -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<xsd:attribute name="other" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="128">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Was Consumer Self-Assessment Completed? -->
	<xsd:element name="consumerSelfAxCompleted">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="TRUE"/>	<!-- Yes -->
				<xsd:enumeration value="FALSE"/>	<!-- No -->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<!-- If the Consumer Self-Assessment was not Completed, why not? (select all that apply) -->
	<xsd:element name="reasonConsumerSelfAxNotCompletedList">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence>
						<xsd:element name="value" minOccurs="0" maxOccurs="unbounded">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value=""/>		<!-- Null -->
									<xsd:enumeration value="CMFLVL"/>	<!-- Comfort Level -->
									<xsd:enumeration value="LOA"/>		<!-- Length of Assessment -->
									<xsd:enumeration value="LIT"/>		<!-- Literacy -->
									<xsd:enumeration value="MHC"/>		<!-- Mental Health Condition -->
									<xsd:enumeration value="PYSCON"/>	<!-- Physical Condition -->
									<xsd:enumeration value="LANG"/>	<!-- Language Barrier -->
									<xsd:enumeration value="OTH"/>		<!-- Other -->
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>

					<xsd:attribute name="otherReason" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="128">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client Name -->
	<xsd:element name="clientName">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- First Name -->
					<xsd:attribute name="first" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Middle Name -->
					<xsd:attribute name="middle" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Last Name -->
					<xsd:attribute name="last" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Preferred Name -->
					<xsd:attribute name="preferred" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client Address -->
	<xsd:element name="clientAddress">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Address Line 1 -->
					<xsd:attribute name="line1" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Address Line 2 -->
					<xsd:attribute name="line2" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- City -->
					<xsd:attribute name="city" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Province -->
					<xsd:attribute name="province" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="AB"/>		<!-- Alberta -->
								<xsd:enumeration value="BC"/>		<!-- British Columbia -->
								<xsd:enumeration value="MB"/>		<!-- Manitoba -->
								<xsd:enumeration value="NB"/>		<!-- New Brunswick -->
								<xsd:enumeration value="NL"/>		<!-- Newfoundland and Labrador -->
								<xsd:enumeration value="NT"/>		<!-- Northwest Territories -->
								<xsd:enumeration value="NS"/>		<!-- Nova Scotia -->
								<xsd:enumeration value="NU"/>		<!-- Nunavut -->
								<xsd:enumeration value="ON"/>		<!-- Ontario -->
								<xsd:enumeration value="PE"/>		<!-- Prince Edward Island -->
								<xsd:enumeration value="QC"/>		<!-- Quebec -->
								<xsd:enumeration value="SK"/>		<!-- Saskatchewan -->
								<xsd:enumeration value="YT"/>		<!-- Yukon -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Postal Code -->
					<xsd:attribute name="postalCode" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="8">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client Phone Number -->
	<xsd:element name="clientPhone">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Phone Number -->
					<xsd:attribute name="number" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Extension -->
					<xsd:attribute name="extension" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="16">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client Email Address -->
	<xsd:element name="clientEmailAddress">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="64">
				</xsd:maxLength>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<!-- Client Date of Birth: YYYY-MM-DDZ note the existence of the letter Z at the end of date value -->
	<xsd:element name="clientDOB">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- DOB Value -->
					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="11"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- DOB Type -->
					<xsd:attribute name="type" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Actual -->
								<xsd:enumeration value="EST"/>		<!-- Estimate -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Health Card Number -->
	<xsd:element name="clientHealthCardInfo">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Health Card Number -->
					<xsd:attribute name="number" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Health Card Version -->
					<xsd:attribute name="version" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="8">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Issuing Territory -->
					<xsd:attribute name="issuingTerritory" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="AB"/>		<!-- Alberta -->
								<xsd:enumeration value="BC"/>		<!-- British Columbia -->
								<xsd:enumeration value="MB"/>		<!-- Manitoba -->
								<xsd:enumeration value="NB"/>		<!-- New Brunswick -->
								<xsd:enumeration value="NL"/>		<!-- Newfoundland and Labrador -->
								<xsd:enumeration value="NT"/>		<!-- Northwest Territories -->
								<xsd:enumeration value="NS"/>		<!-- Nova Scotia -->
								<xsd:enumeration value="NU"/>		<!-- Nunavut -->
								<xsd:enumeration value="ON"/>		<!-- Ontario -->
								<xsd:enumeration value="PE"/>		<!-- Prince Edward Island -->
								<xsd:enumeration value="QC"/>		<!-- Quebec -->
								<xsd:enumeration value="SK"/>		<!-- Saskatchewan -->
								<xsd:enumeration value="YT"/>		<!-- Yukon -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Service Recipient Location (county, district, municipality) -->
	<xsd:element name="serviceRecipientLocation">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value="010-01"/>		<!-- Algoma District -->
				<xsd:enumeration value="010-02"/>		<!-- Brant -->
				<xsd:enumeration value="010-03"/>		<!-- Bruce -->
				<xsd:enumeration value="010-04"/>		<!-- Cochrane District -->
				<xsd:enumeration value="010-05"/>		<!-- Dufferin -->
				<xsd:enumeration value="010-06"/>		<!-- Durham -->
