						<xsd:simpleType>
							<xsd:restriction base="xsd:byte">
								<xsd:enumeration value="-1"/>	<!-- Blank -->
								<xsd:enumeration value="0"/>	<!-- No problem -->
								<xsd:enumeration value="1"/>	<!-- No or moderate problem due to help given -->
								<xsd:enumeration value="2"/>	<!-- Serious problem -->
								<xsd:enumeration value="9"/>	<!-- Not known -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

