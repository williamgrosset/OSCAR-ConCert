						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Address Line 1 -->
					<xsd:attribute name="addressLine1" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Address Line 2 -->
					<xsd:attribute name="addressLine2" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact City -->
					<xsd:attribute name="city" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Province -->
					<xsd:attribute name="province" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="AB"/>		<!-- Alberta -->
								<xsd:enumeration value="BC"/>		<!-- British Columbia -->
								<xsd:enumeration value="MB"/>		<!-- Manitoba -->
								<xsd:enumeration value="NB"/>		<!-- New Brunswick -->
								<xsd:enumeration value="NL"/>		<!-- Newfoundland and Labrador -->
								<xsd:enumeration value="NT"/>		<!-- Northwest Territories -->
								<xsd:enumeration value="NS"/>		<!-- Nova Scotia -->
								<xsd:enumeration value="NU"/>		<!-- Nunavut -->
								<xsd:enumeration value="ON"/>		<!-- Ontario -->
								<xsd:enumeration value="PE"/>		<!-- Prince Edward Island -->
								<xsd:enumeration value="QC"/>		<!-- Quebec -->
								<xsd:enumeration value="SK"/>		<!-- Saskatchewan -->
								<xsd:enumeration value="YT"/>		<!-- Yukon -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Postal Code -->
					<xsd:attribute name="postalCode" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="8">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Phone Number -->
					<xsd:attribute name="phoneNumber" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Phone Extension -->
					<xsd:attribute name="extension" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="16">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Contact Email Address -->
					<xsd:attribute name="emailAddress" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="64">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>


				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Client Capacity (check all that apply) -->
	<xsd:element name="clientCapacity">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element ref="ocan:powerAttorneyPersonalCare"/>
						<xsd:element ref="ocan:powerAttorneyProperty"/>
						<xsd:element ref="ocan:legalGuardian"/>
						<xsd:element ref="ocan:areasOfConcern"/>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Does the client have a Power of Attorney or a substitute decision maker for personal care? -->
	<xsd:element name="powerAttorneyPersonalCare">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Power of Attorney exists? -->
					<xsd:attribute name="personalCare" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Power of Attorney or SDM Name -->
					<xsd:attribute name="name" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Address -->
					<xsd:attribute name="address" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Number -->
					<xsd:attribute name="phoneNumber" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Extension -->
					<xsd:attribute name="extension" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="16">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Does the client have a Power of Attorney for property? -->
	<xsd:element name="powerAttorneyProperty">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Power of Attorney exists? -->
					<xsd:attribute name="property" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Power of Attorney Name -->
					<xsd:attribute name="name" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Address -->
					<xsd:attribute name="address" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Number -->
					<xsd:attribute name="phoneNumber" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Extension -->
					<xsd:attribute name="extension" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="16">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Does the client have a court appointed guardian? -->
	<xsd:element name="legalGuardian">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Legal Guardian exists? -->
					<xsd:attribute name="guardian" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Guardian Name -->
					<xsd:attribute name="name" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Address -->
					<xsd:attribute name="address" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="256"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Number -->
					<xsd:attribute name="phoneNumber" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="32">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Phone Extension -->
					<xsd:attribute name="extension" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="16">
								</xsd:maxLength>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Areas of Concern -->
	<xsd:element name="areasOfConcern">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Finance/Property -->
					<xsd:attribute name="financeProperty" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Treatment Decisions -->
					<xsd:attribute name="treatmentDecisions" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="TRUE"/>	<!-- Yes -->
								<xsd:enumeration value="FALSE"/>	<!-- No -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Age in Years for Onset of Mental Illness -->
	<xsd:element name="ageOnsetMental">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Age in Years -->
					<xsd:attribute name="years" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Age Type -->
					<xsd:attribute name="ageType" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="EST"/>		<!-- Estimate -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Age of first Psychiatric Hospitalization -->
	<xsd:element name="ageHospitalization">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Age in Years -->
					<xsd:attribute name="years" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Age Type -->
					<xsd:attribute name="ageType" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="EST"/>		<!-- Estimate -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- Date when consumer first entered your organization -->
	<xsd:element name="firstEntryDate">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<!-- Year they entered the system -->
					<xsd:attribute name="entryYear" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Month they entered the system -->
					<xsd:attribute name="entryMonth" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
								<xsd:minInclusive value="1"/>
								<xsd:maxInclusive value="12"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<!-- Age Type -->
					<xsd:attribute name="ageType" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value=""/>		<!-- Null -->
								<xsd:enumeration value="EST"/>		<!-- Estimate -->
								<xsd:enumeration value="CDA"/>		<!-- Client Declined to Answer -->
								<xsd:enumeration value="UNK"/>		<!-- Unknown -->
								<xsd:enumeration value="NA"/>		<!-- Not Applicable -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

