	<xsd:element name="OCANDomains">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element ref="ocan:domain" maxOccurs="unbounded"/>
						
						<!-- 01: accommodation -->
						<xsd:element ref="ocan:residenceType" minOccurs="1"/>
						<xsd:element ref="ocan:residenceSupport" minOccurs="1"/>
						<xsd:element ref="ocan:livingArrangementType" minOccurs="1"/>

						<!-- 05: daytime activities -->
						<xsd:element ref="ocan:employStatus" minOccurs="1"/>
						<xsd:element ref="ocan:educationProgramStatus" minOccurs="1"/>

						<!-- 07: psychotic symptoms -->
						<xsd:element ref="ocan:hospitalizedPastTwoYears" minOccurs="1"/>
						<xsd:element ref="ocan:totalAdmissions" minOccurs="1"/>
						<xsd:element ref="ocan:totalHospitalDays" minOccurs="1"/>
						<xsd:element ref="ocan:visitEmergencyDepartment" minOccurs="1"/>
						<xsd:element ref="ocan:communityTreatOrder" minOccurs="1"/>

						<!-- 08: condition and treatment -->
						<xsd:element ref="ocan:diagnosticList" minOccurs="1"/>
						<xsd:element ref="ocan:otherIllnessList" minOccurs="1"/>

						<!-- 20: basic education -->
						<xsd:element ref="ocan:highestEducationLevel" minOccurs="1"/>

						<!-- 23: money -->
						<xsd:element ref="ocan:sourceOfIncome" minOccurs="1"/>
						
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="domain">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence>

						<!-- common for all domains -->
						<xsd:element ref="ocan:needRating" minOccurs="1"/>
						<xsd:element ref="ocan:informalHelpRecvd" minOccurs="1"/>
						<xsd:element ref="ocan:formalHelpRecvd" minOccurs="1"/>
						<xsd:element ref="ocan:formalHelpNeed" minOccurs="1"/>
						<xsd:element ref="ocan:domainComments" minOccurs="1"/>
						<xsd:element ref="ocan:domainActions" minOccurs="1"/>

						<!-- 01: accommodation -->
						<xsd:element ref="ocan:residenceType" minOccurs="0"/>
						<xsd:element ref="ocan:residenceSupport" minOccurs="0"/>
						<xsd:element ref="ocan:livingArrangementType" minOccurs="0"/>

						<!-- 05: daytime activities -->
						<xsd:element ref="ocan:employStatus" minOccurs="0"/>
						<xsd:element ref="ocan:educationProgramStatus" minOccurs="0"/>
						<xsd:element ref="ocan:barriersFindingWorkList" minOccurs="0"/>

						<!-- 06: physical health -->
						<xsd:element ref="ocan:medicalConditionList" minOccurs="0"/>
						<xsd:element ref="ocan:physicalHealthConcern" minOccurs="0"/>
						<xsd:element ref="ocan:concernAreaList" minOccurs="0"/>
						<xsd:element ref="ocan:medicationList" minOccurs="0"/>

						<!-- 07: psychotic symptoms -->
						<xsd:element ref="ocan:hospitalizedPastTwoYears" minOccurs="0"/>
						<xsd:element ref="ocan:totalAdmissions" minOccurs="0"/>
						<xsd:element ref="ocan:totalHospitalDays" minOccurs="0"/>
						<xsd:element ref="ocan:visitEmergencyDepartment" minOccurs="0"/>
						<xsd:element ref="ocan:communityTreatOrder" minOccurs="0"/>
						<xsd:element ref="ocan:psychiatricAdditionalInfo" minOccurs="0"/>
						<xsd:element ref="ocan:symptomList" minOccurs="0"/>

						<!-- 08: condition and treatment -->
						<xsd:element ref="ocan:diagnosticList" minOccurs="0"/>
						<xsd:element ref="ocan:otherIllnessList" minOccurs="0"/>

						<!-- 10: safety to self -->
						<xsd:element ref="ocan:suicideAttempt" minOccurs="0"/>
						<xsd:element ref="ocan:suicideThoughts" minOccurs="0"/>
						<xsd:element ref="ocan:safetyConcernSelf" minOccurs="0"/>
						<xsd:element ref="ocan:safetyToSelfRiskList" minOccurs="0"/>

						<!-- 12: alcohol -->
						<xsd:element ref="ocan:drinkAlcohol" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeAlcohol" minOccurs="0"/>
						<xsd:element ref="ocan:drinkingImpact" minOccurs="0"/>

						<!-- 13: drugs -->
						<xsd:element ref="ocan:drugUseList" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeDrugs" minOccurs="0"/>
						<xsd:element ref="ocan:drugsImpact" minOccurs="0"/>

						<!-- 14: other addictions -->
						<xsd:element ref="ocan:addictionTypeList" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeAddictions" minOccurs="0"/>
